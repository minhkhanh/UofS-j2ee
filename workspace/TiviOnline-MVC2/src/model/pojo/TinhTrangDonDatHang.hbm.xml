<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="model.pojo.TinhTrangDonDatHang" table="tinhtrangdondathang"
        catalog="tivionline">
        <id name="maTinhTrang" type="integer">
            <column name="MaTinhTrang" length="10" />
            <generator class="identity" />
        </id>
        <property name="tenTinhTrang" type="string">
            <column name="TenTinhTrang" length="45" />
        </property>
    </class>
</hibernate-mapping>